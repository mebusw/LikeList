<%@ page language="java" pageEncoding="UTF-8"%>
<%@ taglib uri="http://struts.apache.org/tags-bean" prefix="bean"%> 
<%@ taglib uri="http://struts.apache.org/tags-html" prefix="html"%>
 
<html> 
	<head>
        <meta http-equiv='Content-type' content='text/html;charset=utf-8'/>
        <title>JSP for UserSignupForm form</title>
	</head>
	<body>
		<html:form action="/userSignup" acceptCharset="utf-8" method="get">
			邮箱 : <html:text property="email"/><html:errors property="email"/><br/>
			密码 : <html:password property="password"/><html:errors property="password"/><br/>
                               全名 : <html:text property="fullName"/><html:errors property="fullName"/><br/>
			<html:submit/><html:cancel/>
		</html:form>
	</body>
</html>

