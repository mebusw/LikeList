<div class="add add-friends" id="add-friends" style="display:block;">
  <h4>邀请朋友</h4>
  <p class="tab-head tab-friends-head"><a href="#" rel="tab-top-01">邀请站外的朋友</a><a href="#" rel="tab-top-02">邀请站内的朋友</a></p>
  <div class="tab-art tab-friends-art"> 
    <!--tab-top-01-->
    <div class="add-outweb-friends" id="tab-top-01">
      <ul class="friends-web" id="add-friends-way">
        <li><a href="#" id="163" class="email-163"></a></li>
        <li><a href="#" id="gmail"></a></li>
        <li><a href="#" id="hotmail"></a></li>
        <li><a href="#" id="yahoo"></a></li>
      </ul>
      <p class="p5">登录邮箱，然后给自己的好朋友发邀请邮件，让你的朋友和你一起分享。</p>
      <form id="sendMailForm" action="../action/sendMail.jsp">
        <div class="list-box">
          <ul id="friends-lists">
            朋友列表
          </ul>
        </div>
        <div id="send-email" class="dn">
          <input type="submit" class="btn btn-search" value="点击发送邮件给朋友"/>
        </div>
      </form>
    </div>
    <!--tab-top-02-->
    <div class="add-inweb-friends" id="tab-top-02"></div>
  </div>
  <div class="tr cannel-box"><a href="#" class="cannel">关闭窗口</a></div>
</div>
<div class="help-box hidden" id="help">
  <div class="help-title"></div>
  <div class="help-con"></div>
  <div class="help-canel tr"><a href="#">关闭</a></div>
</div>
<script type="text/javascript" >
$(function(){
	var pageSize = 3;
	$("#add-friends").mytab({tab:"#add-friends"});
	$("#tabs").mytab({tab:"#tabs"});
	var recentLogFeed=new feed(0,pageSize,"#tabs-1 .feed-box","/action/findRecentLog.jsp",1);
	var friendLogFeed=new feed(0,pageSize,"#tabs-2 .feed-box","/action/findFriendLog.jsp",2);
	var recentdealFeed=new feed(0,pageSize,"#tabs-3 .feed-box","/action/findRecentDeal.jsp",3);
	var recentgrFeed=new feed(0,pageSize,"#tabs-4 .feed-box","/action/findRecentGr.jsp",4);
	
	$("#more-recent-log").click(function(e){
		e.preventDefault();
		var offset = parseInt($(this).attr("rel")) + pageSize;
		var recentLogFeedMore=new feed(offset,pageSize,"#tabs-1 .feed-box","/action/findRecentLog.jsp",1);
		$(this).attr("rel", offset);
	})
	
	$("#more-friend-log").click(function(e){
		e.preventDefault();
		var offset = parseInt($(this).attr("rel")) + pageSize;
		var recentLogFeedMore=new feed(offset,pageSize,"#tabs-2 .feed-box","/action/findFriendLog.jsp",2);
		$(this).attr("rel", offset);
	})
	
	$("#more-recent-deal").click(function(e){
		e.preventDefault();
		var offset = parseInt($(this).attr("rel")) + pageSize;
		var recentLogFeedMore=new feed(offset,pageSize,"#tabs-3 .feed-box","/action/findRecentDeal.jsp",3);
		$(this).attr("rel", offset);
	})
	
	$("#more-recent-gr").click(function(e){
		e.preventDefault();
		var offset = parseInt($(this).attr("rel")) + pageSize;
		var recentLogFeedMore=new feed(offset,pageSize,"#tabs-4 .feed-box","/action/findRecentGr.jsp",4);
		$(this).attr("rel", offset);
	})
	
	
})
</script>
