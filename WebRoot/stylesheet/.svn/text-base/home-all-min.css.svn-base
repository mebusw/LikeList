}
.profile-card .icon-wpi{
	background:url(http://a.xnimg.cn/imgpro/icons/pc-wpi.gif) left center no-repeat;
}
.profile-card .icon-gift{
	background:url(http://a.xnimg.cn/imgpro/icons/pc-gift.gif) left center no-repeat;
}
.profile-card .gray .icon-gossip{
	background:url(http://a.xnimg.cn/imgpro/icons/pc-gossip-d.gif) left center no-repeat;
