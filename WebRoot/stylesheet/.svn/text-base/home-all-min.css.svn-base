

/* import by . */
.pymk-header,
.pymk-body .recognize li{
	*zoom:1;
	overflow:hidden;
}
.pymk-header{
	margin-bottom:8px;
}
.pymk-header h4{
	float:left;
	font-weight:bold;
}
.pymk-header .option{
	text-align:right;
}
.pymk-body .recognize{
	*zoom:1;
}
.pymk-body .recognize li{
	margin-bottom:5px;
	position:relative;
}
.pymk-body .recognize p img{
	text-align:text-bottom;
	margin:0;
}


.pymk-body .x-to-hide {
	background: url(http://a.xnimg.cn/imgpro/home/home_icon.png?ver=122) no-repeat scroll -63px -420px;
    width: 14px;
    text-decoration: none;
    height: 14px
}
.pymk-body .x-to-hide:hover {
	background-position:-63px -434px;
	text-decoration:none;
}
.pymk-body .remove-tip {
	position:absolute;
	top:0;
	right:0;
}
.pymk .recognize li span{
	color:#888;
}
.pymk-body .shut:hover{
	background-position:-13px 0;
}
.pymk .recognize li span.headpichold{
	float:left; 
	display:block; 
	width:50px; 
	height:50px; 
	text-align:left; 
	line-height:normal;
	margin:0 5px 0 0;
    overflow: hidden;
}
.pymk .recognize li span.headpichold a{
	height:50px;
	margin:0;
}
.pymk .recognize li span.headpichold img {
    margin: 0;
}
.pymk .recognize li span a {
	display: inline;
	height:14px;
	overflow:hidden;
}
.pymk .recognize li span a.name {
	display: block;
	font-weight: bold;
	width: 100px;
	margin-top:6px;
}
.pymk .recognize li .from{
	display:block;
	height:14px;
	margin-left:5px;
	*margin-left:0;
	*margin-top:4px;
	overflow:hidden;
	width:120px;
	color:#888;
}
.pymk .recognize li p{
	margin:3px 0 3px 55px;
	*margin-left:0;
	line-height:normal;
}
.pymk .comefrom{
	border:1px solid #cee1ee;
	padding:5px 10px;
	background:#f3faff;
	margin-top:5px;
}
.pymk .comefrom p{
	margin-left:0!important;
}
.pymk .comefrom a {
	display: inline;
}
.pymk .page .detail{
	margin-left:55px;
	display:block;
}
.pymk .page li span a.name{
	display:inline;
	float:none;
	width:auto;
}
.pymk .page .headpichold a img{
	width:50px;
	height:50px;
}
.pymk .page .headpichold {
	overflow:hidden;
}

.pymk .pager-back,
.pymk .pager-forward {
	display: inline-block;
	zoom: 1;
	outline: 0 none;
	width: 20px;
	height: 11px;
	overflow: hidden;
	background: url(/images/pager.png) no-repeat;
}
.pymk .pager-back {
	background-position: 0 -11px;
}
.pymk .pager-forward {
	background-position: -20px -11px;
}
.pymk .pager-back:hover {
	background-position: 0 -22px;
}
.pymk .pager-forward:hover {
	background-position: -20px -22px;
}
.pymk .pager-back-disable,
.pymk .pager-back-disable:hover {
	background-position: 0 0;
}
.pymk .pager-forward-disable,
.pymk .pager-forward-disable:hover {
	background-position: -20px 0;
}
.pymk .profile-label {
	color: #666;
	font-weight: bold;
}

.pymk-body .reconnect-gossip {
    clear: both;
    background: #F0F5F8;
    padding: 4px;
    border-style: solid;
    border-width: 1px;
    border-color: #E3EEF8 #E3EEF8 #CEE1EE;
    margin-top: 3px;
}
.pymk-body .reconnect-gossip textarea {
    margin: 0;
    width: 202px;
}
.pymk-body .reconnect-gossip .clearfix {
    text-align: right;
}
.pymk-body .reconnect-gossip .clearfix label {
    font-weight: normal;
    color: #333;
    margin-right: 5px;
}
.pymk-body .reconnect-gossip .clearfix label input {
    margin-right: 2px;
}


/* import by ../../../core/trunk/core/feed/feed.css */
/*
 * 一条新鲜事
 */
.a-feed {
	position: relative;
	padding-bottom: 5px;
	margin-bottom: 10px;
	word-wrap: break-word;
	border-bottom: 1px solid #E3EEF8;
	behavior: expression(function(ele){ele.runtimeStyle.behavior='none';Expressions.pseudo.hover(ele, 'a-feed_hover');}(this));
	*zoom: 1;
}
.a-feed:after {
	content: '\20';
	display: block;
	clear: both;
}
.a-feed aside {
	float: left;
	margin-right: 10px;
	_display: inline;
	_margin-right: 7px;
}
.a-feed aside a,
.a-feed aside img {
	display: block;
}
.a-feed h3 {
	margin-left: 50px;
	margin-right: 240px;
	line-height: 1.4em;
	font-size:12px;
	padding-bottom:5px;
	_margin-left: 57px;
}
.a-feed h3 a.text {
	color: #333;
}
.a-feed h3 a.text:hover {
	text-decoration: none;
}

.a-feed h3 img {
	vertical-align: text-bottom;
	margin: 0 0.2ex;
}


.a-feed a.text {
	color: #888;
}
.a-feed a.text:hover {
	text-decoration: none;
}
.a-feed .details {
	margin-left: 50px;
	margin-bottom: 5px;
}
.a-feed .details .legend {
	line-height: 1.5em;
}
.a-feed .details .legend span.duration {
	margin-right: 2.9px;
}
.a-feed .details .legend span.duration  a{
	color:#888;display:inline-block;
}

.a-feed .details .legend span.duration  a:hover,.a-feed .details .legend span.duration  a:focus,.a-feed .details .legend span.duration  a:active{color:#000;}
.a-feed .details .legend span.from a {
	color: #808080;
}

.a-feed .status-lbs span,
.a-feed .status-lbs a {
	display: inline-block;
	zoom: 1;
	height: 17px;
    *font-family: simsun;
}
.a-feed .status-lbs a {
    *position: relative;
    _position: static;
    *top: -2px;
}

.a-feed .details .legend span.seperator {
	color: #4281C4;
}
.a-feed .details img {
	vertical-align: text-bottom;
}
.a-feed .details span.duration {
	color: #888;
}


.a-feed .content {
	position: relative;
	margin-left: 50px;

	color: #333;
	line-height: 1.6em;
	_margin-left:57px;
	*zoom: 1;
	overflow: hidden;
}
.a-feed .rightcontent {
	position: relative;
	margin-left: 125px;
	color: #333;
	line-height: 1.6em;
	_margin-left:57px;
	*zoom: 1;
}
/* webkit hack */
@media screen and (-webkit-min-device-pixel-ratio:0) {
    .a-feed {
        min-height: 50px;
    }
    .a-feed aside .newsfeed-user {
        width: 50px;
        height: 50px;
        overflow: hidden;
        position: absolute;
    }
	.a-feed .content {
		overflow: visible;
	}
    .a-feed .content:after {
        content: "\20";
        display: block;
        clear: both;
    }
}
/* .a-feed .content:after {
	content: "\20";
	display: block;
	clear: both;
} */
.a-feed .content ul.grid.thumbnails,
.a-feed .content ul.hotshare,
.a-feed .content ul.hotshare-lead {
	*zoom: 1;
}
.a-feed .content ul.grid.thumbnails:after,
.a-feed .content ul.hotshare:after,
.a-feed .content ul.hotshare-lead:after,
.a-feed .content ul.hotvideo:after {
	content: "\20";
	display: block;
	clear: both;
}
.a-feed .content ul.grid.thumbnails li,
.a-feed .content ul.hotshare li,
.a-feed .content ul.hotshare-lead:after {
	float: left;
	padding: 3px;
	margin-right: 10px;
	margin-bottom: 10px;
	vertical-align: top;
	border: 1px solid #bbc9e2;
	*width: auto;
}
.a-feed .content ul.grid.thumbnails li a img {
	display: block;
	background-position: center center;
	background-repeat: no-repeat;
}
.a-feed .content .photo-item {
    display: inline;
    float: left;
    width: 118px;
    padding-bottom: 5px;
    color: #888;
}
.a-feed .content i.photo,
.a-feed .content img.photo {
    float:left;
    padding: 3px;
    margin-right: 10px;
    margin-bottom: 5px;
    vertical-align: top;
    border: 1px solid #bbc9e2;
}
.a-feed .content .photo-item span.photo-info {
    display:block;
    width:108px;
    over-flow:hidden;
    color: #888;
}
.a-feed .content a.more {
	position: absolute;
	top: 50%;
	left: 350px;
	margin-top: -15px;
	width: 28px;
	height: 29px;
	margin-left: 10px;
	text-indent: -9999px;
	background: url(http://a.xnimg.cn/imgpro/home/home_icon.png?ver=100310) no-repeat -27px -420px;
	outline: none;
}
.a-feed .content a.more:hover {
	background-position: 0 -420px;
}
.a-feed .content q,
.a-feed .content .pagecomment {
	line-height: 1.5em;
	*zoom: expression(function(ele) {
		ele.style.zoom = "1";
		Expressions.after(ele);
		Expressions.before(ele);
	}(this));
}
.a-feed .content q:before,
.a-feed .content q before,
.a-feed .content .pagecomment:before,
.a-feed .content .pagecomment before
{
	display: inline-block;
	width: 10px;
	height: 7px;
	margin-right: 0.5ex;
	vertical-align: top;
	background: url(http://a.xnimg.cn/imgpro/home/home_icon.png?ver=100310) no-repeat 0 -333px;
	_font-size: 0;
}
.a-feed .content .pagecomment:before,
.a-feed .content .pagecomment before {
	float:left;
}
.a-feed .content q:after,
.a-feed .content q after,
.a-feed .content .pagecomment:after,
.a-feed .content .pagecomment after {
	display: inline-block;
	width: 10px;
	height: 7px;
	margin-left: 0.5ex;
	background: url(http://a.xnimg.cn/imgpro/home/home_icon.png?ver=100310) no-repeat -10px -333px;
	_font-size: 0;
}
.a-feed .no-interact{float:left;width:420px}
.a-feed .no-interact-pic{float:right;margin-right:40px;display:inline;}
.a-feed .no-interact-pic img{display:block}
.a-feed .content .pagecomment p {
	display:inline;
}
.a-feed .content .pagecomment p.citation{
	display:inline-block;
	*display:block;
	width:450px;
	*width:auto;
}
.a-feed .content .pagecomment after {
	/*float:right;*/
}



/**
 * 新鲜事基本布局
 */
.feed-module .feed-header {
	border-bottom: 1px solid #A0BEDC;
	margin-bottom: 10px;
	*zoom: 1;
}
.feed-module .feed-header:after {
	content: '\20';
	display: block;
	clear: both;
}

/**
 * 单栏新鲜事，与feed-header二选一
 */
.feed-module .feed-headline {
	background-color: #F7F7F7;
	padding: 0 10px;
	border-top: 1px solid #DDDDDD;
	*zoom: 1;
}
.feed-module .feed-headline:after {
	content: '\20';
	display: block;
	clear: both;
}
.feed-module .feed-headline h3 {
	float: left;
	font-weight: bold;
	line-height: 28px;
}

/**
 * 新鲜事加载中
 */
.feed-module .feed-loading {
	padding: 10px;
	margin-bottom: 10px;
	text-align: center;
	font-size: 14px;
	border: 1px solid #CEE1EE;
	background-color: #F0F5F8;
}

/**
 * 没有更多新鲜事
 */
.feed-module .nomore-feed {
	padding: 10px;
	text-align: center;
	font-size: 14px;
	border: 1px solid #CEE1EE;
	background-color: #F0F5F8;
}

/**
 * 更多新鲜事按钮
 */
.feed-module .more-feed a {
	display: block;
	padding: 10px;
	text-align: center;
	font-size: 16px;
	font-weight: bold;
	border: 1px solid #CEE1EE;
	background-color: #F0F5F8;
}
.feed-module .more-feed a:hover {
	background-color: #E3EEF8;
	text-decoration: none;
}

/**
 * 全部标记
 */
.feed-module .mark-all-read a {
	color: #888;
}
/*
 * 新鲜事类型tab
 */
.feed-module .feed-types {
	float: left;
}
.feed-module .feed-types ul.tab {
	padding-left: 10px;
	*float: left;
}
.feed-module .feed-types ul.tab:after {
	content: '\20';
	display: block;
	clear: both;
}
.feed-module .feed-types ul.tab li {
	float: left;
	margin-right: 5px;
}
.feed-module .feed-types ul.tab li.selected {
	/* 盖住下面一条线 */
	margin-bottom: -1px;
	*position: relative;
}
.feed-module .feed-types ul.tab li a {
	display: block;
	padding: 0 10px;
	line-height: 25px;
	outline: 0 none;
	background-color: #E3EEF8;
	text-decoration: none;
}
.feed-module .feed-types ul.tab li a:hover {
	background-color: #CEE1EE;
}
.feed-module .feed-types ul.tab li.selected a {
	color: #333;
	background-color: #FFF;
	border-left: 1px solid #A0BEDC;
	border-right: 1px solid #A0BEDC;
	border-top: 1px solid #A0BEDC;
}

/**
 * 未读数
 */
.feed-module .unreads-count {
	display: inline-block;
	padding: 0 3px;
	margin-left: 2px;
	font-size: 11px;
	font-weight: bold;
	color: #fff;
	background-color: #84AEC6;
	border-radius: 3px;
	line-height: normal;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	_float: left;
	_margin-left: 63px;
	_margin-top: -19px;
	_margin-right: 10px;
	*zoom: 1;
}
.feed-module a:hover .unreads-count {
	background-color:#42719C;
}

/**
 *三频道切换工具
 */
.feed-module .feed-tools {
	padding: 0;
	float: left;
	height: 20px;
	line-height: 20px;
	margin-top: 3px;
}
.feed-module .feed-tools li {
	float: left;
	margin-left: 20px;
	list-style-type: none;
}
.feed-module .feed-tools li li {
	float: none;
	margin-left: 0;
}
.feed-module .feed-tools a:link,
.feed-module .feed-tools a:visited {
	color: #005EAC;
}

/**
 * 新鲜事分类过滤器
 */
.feed-module .category-filter {
	float: left;
	*position: relative;
	*z-index: 1;
}
* html .feed-module .category-filter {
	behavior: expression(function(ele){ele.style.behavior='none';Expressions.pseudo.hover(ele, 'category-filter_hover')}(this));
}
.feed-module .category-filter:hover,
.feed-module .category-filter_hover {
	border-width: 1px 1px 0;
	border-style: solid solid none;
	border-color: #A0BEDC #A0BEDC #fff;
	margin: -1px -11px -1px -6px;
}


.feed-module .category-filter menu {
	display: none;
	position: absolute;
	margin-left: -1px;
	border: 1px solid #A0BEDC;
	background: #fff;
	overflow: hidden;
	padding: 2px 0;
	z-index: 1;
	*width: 90px; /* IE6 太宽 */
}
.feed-module .category-filter:hover menu,
.feed-module .category-filter_hover menu {
	display: block;
}
.feed-module .category-filter menu a {
	display: block;
	padding: 0 20px 0 20px;
	height: 20px;
	line-height: 20px;
	outline: 0 none;
	white-space: nowrap;
}
.feed-module .category-filter menu a:hover {
	text-decoration:none;
	background-color:#558BC6;
	color:#fff;
}
.feed-module .category-filter menu li.selected a {
	color: #333;
	font-weight: bold;
	background-image: url(images/newsfeed-type-filter-selected.png);
	background-repeat: no-repeat;
	background-position: 6px center;
}
.feed-module .category-filter menu li.selected:hover a {
	background-color:#fff;
}

/**
 * 刷新新鲜事
 */
.feed-module .reload-feed {
	display: block;
	background: url(../images/reload.png) left center no-repeat;
	padding-left: 15px;
}

/**
 * 设置新鲜事
 */
.feed-module .feed-setting {
	display: block;
	padding-left: 18px;
	background: url(../images/reload.png) no-repeat left center;
}

/**
 * 新鲜事列表
 */
.feed-module .feed-list {
	margin-bottom: 10px;
	*zoom: 1;
}
.feed-module .a-feed {
	position: relative;
	padding-bottom:6px;
	margin-top:10px;
	border-bottom: 1px solid #E3EEF8;
}
.feed-module .a-feed img,
.feed-module p img {
	margin: 0;
}


/**
 * 视频新鲜事合并
 */
.feed-module .a-feed .content .video-combine {
	font-size: 0;
	letter-spacing: -4px;
	height: 126px;
	overflow: hidden;
	*position: relative;
}
.feed-module .a-feed .content .video-combine .video-box {
	display: inline-block;
	*display: inline;
	*zoom: 1;
	font-size: 12px;
	letter-spacing: 0;
	width: 120px;
	margin-right: 30px;
	margin-bottom: 10px;
	vertical-align: top;
}
.feed-module .a-feed .content .video-combine .video-box .caption {
	height: 30px;
	line-height: 15px;
	overflow: hidden;
}
.feed-module .a-feed .content .video-combine a.more {
	margin: 0;
	left: auto;
	right: 12px;
	top: 32px;
}


/* import by . */
.profile-card{
	position:absolute;
	width:305px;
	word-wrap:break-word;
	*word-wrap:normal;
	word-break:break-all;
}
.profile-card p img,.profile-card li img,.profile-card img{
	margin:0;
}
.profile-card .shadow{
	*background-color:#CCC;
	zoom:1;
}
.profile-card .shadowed{
	border:1px solid #AAA;
	background-color:#FFF;
	*position:relative;
	*left:-3px;
	*top:-3px;
	box-shadow:3px 3px 2px #ccc;
	-moz-box-shadow:3px 3px 2px #ccc;
	-webkit-box-shadow:3px 3px 2px #ccc;
	zoom:1;
