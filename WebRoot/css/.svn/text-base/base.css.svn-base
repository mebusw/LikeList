.group-btn{ background:url(../images/btn-01.png) no-repeat; position:absolute; left:-75px; top:0px; height:95px; width:295px; overflow:hidden; zoom:1;}
.now-price{ display:block; float:left; width:130px; height:75px; padding-left:20px; font-size:36px; font-weight:900; color:#FFF; line-height:70px; text-align:center;}
.btn-buy{ float:left; display:block; background:url(../images/btn_buy.png) no-repeat left 5px; height:62px; width:132px; padding-top:8px;}
.group-save{ padding-top:85px; overflow:hidden; zoom:1; color:#000; padding-left:7px;}
.group-save p{ font-size:16px; font-weight:700; line-height:30px; width:69px; text-align:center; float:left;}
.group-time{font-size: 14px;padding: 15px 0px 0px 10px;border: 1px solid #FBD686;background: #FFF8E8;margin-top: 10px;padding-bottom: 10px;}
.group-purchaser{font-size: 14px;padding: 15px 0px 0px 10px;border: 1px solid #FBD686;background: #FFF8E8;margin-top: 10px;padding-bottom: 10px;}
.group-succeed{ background:url(../images/buy-succ.gif) no-repeat; text-indent:35px; color:#F00; text-align:center; padding:0 0 5px 0; font-weight:700;}
.group-poster{ font-size:16px; text-align:center; line-height:30px; font-weight:700;}
.group-address{ background:#F3F3F3; line-height:24px; text-indent:20px;}


/*friends-list*/
.friends-list{ padding:10px;}
.friends-list li{ margin-bottom:5px; overflow:hidden; zoom:1; border-bottom:#efefef solid 1px; padding-bottom:4px;} 
.friends-list li a,.friends-list li img{ float:left;}
.friends-list li img{ margin-right:10px;}

/*help*/
.help-box{position:absolute; width:200px; overflow:hidden; left:163px; top:200px; border:#EFEFEF solid 2px; z-index:999; background:#fff;}
.help-box-highlight{ background:#FFECEC; border-color:#FFECEC;}
.help-hd{ line-height:20px; padding:0 3px; background:#EFEFEF;}
.help-con{ padding:10px;}
.help-canel{padding:6px 0 3px 0; background:#EFEFEF;}

#likeNav span{ cursor:pointer;}

.lbox{ padding:6px;}
.lbox li{ float:left; width:64px; padding:4px; text-align:center; height:75px;}
.lbox li a{display:block;text-align:center;}
.lbox li a img,.lbox li a span{ display:block; overflow:hidden; margin:0 auto;}
.more-btn{ float:none; text-align:right; clear:both; padding:0px 10px;}

#aside-friends-box{ padding:6px; background:#fff; width:400px;}


/*deal*/
.deal{ padding:10px;}
.deal h2{color:#03C;}
.deal-con{ margin-bottom:4px;}
.deal-time{}
.deal-detail{ padding-top:10px; float:left; width:400px;}
.deal-img{ width:220px; float:left; text-align:right;}

/*jQuery UI Tabs 1.8.11*/
.ui-tabs { position: relative; width:94%; margin:0 auto; margin-bottom:50px;} /* position: relative prevents IE scroll bug (element with position: relative inside container with overflow: auto appear as "fixed") */
.ui-tabs .ui-tabs-nav { overflow:hidden; zoom:1; background:url(../images/bor-01.png) repeat-x left bottom;}
.ui-tabs .ui-tabs-nav li { float:left; margin-right:5px; border:#c7cacd solid 2px;  border-bottom:none;
	-moz-border-radius-topleft: 5px;
	-moz-border-radius-topright: 5px;
	-webkit-border-top-right-radius: 5px;
	-webkit-border-top-left-radius: 5px;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px; }
.ui-tabs .ui-tabs-nav li a { background:url(../images/tab-bg.png) repeat-x left bottom; display:block; height:32px; line-height:32px; padding:0 15px; outline:none; }
.ui-tabs .ui-tabs-nav li.ui-tabs-selected { margin-bottom: 0; padding-bottom: 1px; }
.ui-tabs .ui-tabs-nav li.ui-tabs-selected a{ cursor: text; }
.ui-tabs .ui-tabs-nav li.ui-state-active a{background:url(../images/tab-bg.png) repeat-x left top; color:#333;}
.ui-tabs .ui-tabs-nav li a, .ui-tabs.ui-tabs-collapsible .ui-tabs-nav li.ui-tabs-selected a { cursor: pointer; } /* first selector in group seems obsolete, but required to overcome bug in Opera applying cursor: text overall if defined elsewhere... */
.ui-tabs .ui-tabs-panel { display: block; border-width: 0; padding: 1em 1.4em; background: none; }
.ui-tabs .ui-tabs-hide { display: none !important; }

/*search-nav*/
.search-nav { padding:0 15px;}
.search-nav a{ display:inline-block; height:20px; width:14px; text-align:center; background:#fcfcfc; line-height:20px; margin-right:3px;}
.search-nav a.active{ background:#036; color:#fff;}

