.tab-friends-head a.cur{ background:#FFF;}
.tab-friends-art{border:#D2D2D2 solid 1px; background:#FFF; padding:8px 0;}

/*search*/
.main-box{ width:94%; padding:10px 0; margin:5px auto; background:#fff; border:#C6C6C6 solid 1px;
	    -moz-border-radius: 5px;
		-webkit-border-radius: 5px;
		border-radius: 5px;
}
