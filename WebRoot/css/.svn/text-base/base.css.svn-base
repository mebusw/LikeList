.nobor{ border:none !important;}

/*a-button*/
.btn{ margin:3px; position:relative; zoom:1; display:inline-block; cursor:pointer;}
.btn:hover{ text-decoration:none; outline:none;}
.btn-text{display:inline-block; padding:0 16px;}

.btn-blue{background:url(../images/tab_dark_blue_back.png) repeat-x left top; border:none; border-radius:4px; color:#fff; height:24px; line-height:24px;}
.btn-blue:hover{background:url(../images/tab_dark_blue_back.png) repeat-x left bottom; color:#fff;}

.btn-gray-a{background:url(../images/search-btn.png) repeat-x left top; border-radius:4px; height:28px; line-height:28px;border:#e3e4e4 solid 1px;}
.btn-gray-a:hover{background:url(../images/search-btn.png) repeat-x left bottom; color:#666; border-color:#666;}
/*reg-email*/
.reg-email{ background:#CCC; border:#21496B solid 1px; padding:1px; border-radius:4px;}
.reg-email .btn-text{ height:27px; background:url(../images/tab_dark_blue_back.png) repeat-x left top;color:#fff; line-height:26px;border-radius:4px;}
.reg-email:hover .btn-text{background:url(../images/tab_dark_blue_back.png) repeat-x left bottom;}
/*input-button*/
.input-btn{background:url(../images/tab_dark_blue_back.png) repeat-x left top; border:none;border-radius:4px; height:24px; width:100px; line-height:14px; cursor:pointer; color:#fff;}
.input-btn:hover{background:url(../images/tab_dark_blue_back.png) repeat-x left bottom;}
/*btn-search*/
.btn-search{ height:26px; line-height:26px; background:url(../images/search-btn.png) repeat-x left top; padding:0 16px; margin-top:2px; cursor:pointer; border:#e3e4e4 solid 1px;}
.btn-search:hover{ background:url(../images/search-btn.png) repeat-x left bottom; }
/*btn-gray*/
.btn-input-gray{ height:26px; line-height:26px; background:url(../images/search-btn.png) repeat-x left top; padding:0 16px; cursor:pointer; border:#e3e4e4 solid 1px;}
.btn-input-gray:hover{background:url(../images/search-btn.png) repeat-x left bottom;}

.btn-help{ background: -webkit-gradient(linear,left top,left bottom,from(rgba(245,245,245,1)),to(rgba(200,200,200,1))); 
 background: -moz-linear-gradient(top,rgba(245,245,245,1),rgba(200,200,200,1));border:#gb(220,220,220) solid 1px; border-radius:3px; margin:5px;}
/*global*/
body{ color:#545454; font:12px/1.5 "宋体";}
a{ color:#9a9a9a; text-decoration:none}
a:hover{ color:#333; text-decoration:none;}
.fm_blue{ color:#03C;}
/*form*/
.text-input{
	-webkit-border-radius: 4px;
	-moz-border-radius: 4px;
	border-radius: 4px;
	padding: 6px 6px 4px;
	line-height: 14px;
	background: white;
	color: #202122;
	margin: 2px 0;
}
.index-p select{ width:150px;-webkit-border-radius: 4px;-moz-border-radius:4px;border-radius: 4px; height:20px; border:#999 solid 1px;}


/*photo-box*/
.photo-box{ display:inline-block;}
.photo-box img{ display:block;}
.photo-shop-trade{ padding:3px; border:#c6c6c6 solid 2px;}

/*tab*/
/*tab-main*/
.tab-main{ width:94%; margin:0 auto; margin-bottom:50px;}
.tab-main .tab-main-head{ overflow:hidden; zoom:1; background:url(../images/bor-01.png) repeat-x left bottom;}
.tab-main .tab-main-head li{ float:left; margin-right:5px; border:#c7cacd solid 2px;  border-bottom:none;
	-moz-border-radius-topleft: 5px;
	-moz-border-radius-topright: 5px;
	-webkit-border-top-right-radius: 5px;
	-webkit-border-top-left-radius: 5px;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
}
.tab-main .tab-main-head li a{ background:url(../images/tab-bg.png) repeat-x left bottom; display:block; height:32px; line-height:32px; padding:0 15px; outline:none;}
.tab-main .tab-main-head li a.cur{ background:url(../images/tab-bg.png) repeat-x left top; color:#333;}
.tab-main .tab-main-head li a.stress { background:#630; color:#fff;}
.tab-main .tab-main-art{ background:#fff; padding:10px; border:#c7cacd solid 2px;  border-top:none;
    -moz-border-radius-bottomleft: 5px;
	-moz-border-radius-bottomright: 5px;
	-webkit-border-bottom-left-radius: 5px;
	-webkit-border-bottom-right-radius: 5px;
	border-bottom-left-radius: 5px;
	border-bottom-right-radius: 5px;
}
/*tab-msg*/
.tab-msg-head{ font-weight:500; color:#176BC1;}
.tab-msg-head a{ display:inline-block; padding:0 5px; color:#545454;}
.tab-msg-head a.cur{color:#F68A33;}
