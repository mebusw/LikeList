/*list*/


/*header*/
.header{ width:100%; position:relative; z-index:1; margin-bottom:10px; background:#fff url(../images/top-bg02.png) repeat-x left bottom;}
.header-con{width:980px; margin:0 auto; overflow:hidden; zoom:1; position:relative; padding:5px;}
