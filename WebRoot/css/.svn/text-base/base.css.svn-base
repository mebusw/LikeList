/* CSS Document */
/*reset*/
html,body,ul,ol,h1,h2,h3,h4,h5,h6,blockquote,p,dl,dt,dd{padding:0;margin:0;}
table{border-collapse:collapse;border-spacing:0;}
b,i{ font-weight:normal;}
img {border:none;}
ol,ul {list-style:none;}
article,aside,section{ display:block;}
